{
  "name": "index-of-newline",
  "version": "1.1.8",
  "description": "Find indexOf CR, LF or CRLF",
  "keywords": [
    "batch",
    "data",
    "fetch",
    "form",
    "multipart",
    "parts",
    "typescript"
  ],
  "homepage": "https://github.com/kmalakoff/index-of-newline#README.md",
  "bugs": {
    "url": "https://github.com/kmalakoff/index-of-newline/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/index-of-newline.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kevin Malakoff",
    "email": "kmalakoff@gmail.com",
    "url": "https://github.com/kmalakoff"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "unpkg": "dist/umd/index-of-newline.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds test:node && tsds test:browser",
    "test:engines": "nvu engines tsds test:node && tsds test:browser",
    "version": "tsds version"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.17",
    "ts-dev-stack": "^0.9.10"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
